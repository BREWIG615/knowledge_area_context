%% for i in range(rows | length)

    \VAR{format_element(rows[i], 
    goals[i], 
    inputs[i], 
    activities[i], 
    deliverables[i],
    suppliers[i],
    participants[i],
    consumers[i],
    techniques[i],
    tools[i],
    metrics[i])}

%% endfor

\end{document}